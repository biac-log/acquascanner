{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      false,
      "tabs",
      2
    ],
    "interface-name": false,
    "no-consecutive-blank-lines": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": false,
    "trailing-comma": false,
    "curly": false
  }
}